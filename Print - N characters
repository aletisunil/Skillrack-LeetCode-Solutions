import java.util.*;
public class Hello {

    public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		String s=new String("abcd");
		int n1=n/4;
        int n2=n%4;
        for(int i=0;i<n1;i++){
            System.out.print("abcd");
        }
        for(int i=0;i<n2;i++){
            System.out.print(s.charAt(i));
        }
	}
}
