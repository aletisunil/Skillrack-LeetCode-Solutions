import java.util.*;
public class Hello {

    public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in);
		long n=sc.nextLong();
		String s=Long.toString(n);
		char[]c=s.toCharArray();
		for(int i=0;i<c.length;i++)
		{
		    int k=Character.getNumericValue(c[i]);
		    System.out.print(k*k*k);
		}

	}
}
