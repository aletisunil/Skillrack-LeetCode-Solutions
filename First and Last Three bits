import java.util.*;
public class Hello {

    public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		String s=Integer.toBinaryString(n);
		String s1="";
		String s2="";
		for(int i=0;i<3;i++)
		{
		    s1=s1+s.charAt(i);
		}
		for(int i=s.length()-3;i<s.length();i++)
		{
		    s2=s2+s.charAt(i);
		}
		System.out.print(Integer.parseInt(s1,2)+" ");
		System.out.print(Integer.parseInt(s2,2));
		}
}
