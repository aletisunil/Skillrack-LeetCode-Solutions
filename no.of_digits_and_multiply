import java.util.*;
public class Hello {

    public static void main(String[] args) {
		//Your Code Here
       Scanner sc=new Scanner(System.in);
       int n=sc.nextInt();
       int count=0;
       int r;
       int n1=n;
       while(n1>0)
       {
           r=n1%10;
           count++;
           n1=n1/10;
       }
       System.out.print(n*count);

	}
}
