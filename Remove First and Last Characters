import java.util.*;
public class Hello {

    public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		String[] s=new String[n];
		for(int i=0;i<n;i++)
		{
		    s[i]=sc.next();
		}
		for(int i=0;i<n;i++)
		{
		    char[] c=s[i].toCharArray();
		    for(int j=1;j<c.length-1;j++)
		    {
		        System.out.print(c[j]);
		    }
		    System.out.println();
		}

	}
}
