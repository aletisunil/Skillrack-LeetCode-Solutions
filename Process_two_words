import java.util.*;
public class Process_two_words {

    public static void main(String[] args) {
		//Your Code Here
		Scanner sc=new Scanner(System.in);
		String s=sc.nextLine();
		char[] c=s.toCharArray();
		
		for(int i=0;i<c.length;i++)
		{
		    for(int j='a';j<='z';j++)
		    {
		    if(c[i]==j)
		    {
		        char k=(char) 'z'-j+1;
		        System.out.print(k);
		}
		    }

	}
    }
