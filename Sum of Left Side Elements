import java.util.*;
public class Hello {

    public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int[] a=new int[n];
		for(int i=0;i<n;i++)
		{
		    a[i]=sc.nextInt();
		}
		System.out.print('0'+" ");
		for(int i=1;i<n;i++)
		{
		    int sum=0;
		    for(int j=0;j<i;j++)
		    {
		        sum=sum+a[j];
		    }
		       System.out.print(sum+" ");
		    
		}
	}
}
