import java.util.*;
public class Hello {

    public static void main(String[] args) {
		Scanner sc= new Scanner(System.in);
		String s1=sc.nextLine();
	    String s2=sc.nextLine();
	    String[] s3=s1.split(" ");
	    String[] s4=s2.split(" ");
        for(int i=0;i<s4.length;i++)
		{
		    if(s3[0].equals(s4[i]))
		    {
		        System.out.print(s3[1]+" ");
		    }
		    else
		    {
		        System.out.print(s4[i]+" ");
		    }
		}
	}
}
