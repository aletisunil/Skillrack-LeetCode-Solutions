import java.util.*;
public class Hello {

    public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    int[] a =new int[n];
    for(int i=0;i<n;i++){
        a[i]=sc.nextInt();
    }
    int sum=0;
    int x= sc.nextInt();
    int y=sc.nextInt();
    for(int i=0;i<n;i++){
        if(a[i]>=x && a[i]<=y){
            sum+=a[i];
        }
    }
    System.out.print(sum);

	}
}
