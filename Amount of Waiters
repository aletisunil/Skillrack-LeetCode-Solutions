import java.util.*;
public class Hello {

    public static void main(String[] args) {
	  Scanner sc=new Scanner(System.in);
		int w=sc.nextInt();
		int n=sc.nextInt();
		int[]a=new int[n];
		for(int i=0;i<n;i++)
		{
		    a[i]=sc.nextInt();
		}
		int[] p=new int[w];
		int k=0;
		for(int i=0;i<n;i++)
		{
		    k=i%w;
		    p[k]=p[k]+a[i];
		}
		for(int i=0;i<p.length;i++)
		{
		    System.out.print(p[i]+" ");
		}

	}
}
