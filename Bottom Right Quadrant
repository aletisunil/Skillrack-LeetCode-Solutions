import java.util.*;
public class Hello {

    public static void main(String[] args) {
		//Your Code Here
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int[][] a=new int[n][n];
		for(int i=0;i<n;i++)
		{
		    for(int j=0;j<n;j++)
		    {
		        a[i][j]=sc.nextInt();
		    }
		}
		for(int i=n/2;i<n;i++)
		{
		    for(int j=n/2;j<n;j++)
		    {
		        System.out.print(a[i][j]+" ");
		    }
		    System.out.println();
		}

	}
}
